                "Using",
                "Installing",
                "Support"
            ],
            "overview": {
                "post": "bin/readme/overview-body.md"
            },
            "installing": {
                "body": "bin/readme/installing-body.md"
            },
            "support": {
                "body": "https://raw.githubusercontent.com/runcommand/runcommand-theme/master/bin/readme-partials/support-premium.md"
            },
            "show_powered_by": false
